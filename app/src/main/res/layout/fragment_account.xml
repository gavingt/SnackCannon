    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.storefront.account.AccountFragment">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="AccountFragment"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/change_address_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="change address"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textView" />

        <Button
            android:id="@+id/change_contact_info_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="change contact info"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/change_address_button" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>